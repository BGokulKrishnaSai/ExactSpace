# React Native Posts App - Environment Variables

# API Configuration
REACT_NATIVE_API_URL=https://jsonplaceholder.typicode.com

# Storage Configuration
STORAGE_PERSISTENCE_ENABLED=true
MAX_SEARCH_HISTORY=20

# App Configuration
APP_NAME=PostsHub
APP_VERSION=1.0.0

# Feature Flags
ENABLE_ANIMATIONS=true
ENABLE_SKELETON_LOADING=true
ENABLE_PULL_TO_REFRESH=true

# Debugging
DEBUG_MODE=false
LOG_API_CALLS=false
